{
  "name": "raven",
  "displayName": "Raven",
  "description": "Raven Programming Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "rv",
      "aliases": ["Raven", "rv"],
